{
  "name": "react-webpack-starter",
  "version": "1.0.0",
  "description": "A webpack React starter",
  "main": "server/server.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --hot --inline",
    "server": "node-dev server/server",
    "test": "ava --watch"
  },
  "ava": {
    "failfast": true,
    "tap": true,
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ]
    }
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "ava": "^3.7.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "copy-webpack-plugin": "^4.6.0",
    "enzyme": "^3.11.0",
    "node-dev": "^4.0.0",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "superagent": "^5.1.3"
  }
}
